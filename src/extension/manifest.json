{
  "name": "Speed Changer",
  "version": "0.1",
  "manifest_version": 2,
  "author": "@raerpo",
  "description": "Change the speed of Youtube videos and remember your preferences",
  "homepage_url": "https://github.com/raerpo/speed-changer",
  "icons": {
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    },
    "default_popup": "popup/popup.html",
    "default_title": "Speed Changer"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://*/*?*",
    "http://*/*?*"
  ]
}
